{
  "include": [
    "src"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "venv",
    ".venv"
  ],
  "venvPath": ".",
  "venv": "venv",
  "reportMissingImports": true,
  "reportMissingTypeStubs": true,
  "reportUnknownMemberType": false,
  "reportUnknownParameterType": false,
  "reportUnknownVariableType": false,
  "pythonVersion": "3.13",
  "typeCheckingMode": "basic"
}